#
# MMC subsystem configuration
#

menuconfig MMC
	tristate "MMC/SD/SDIO card support"
	depends on HAS_IOMEM
	help
	  This selects MultiMediaCard, Secure Digital and Secure
	  Digital I/O support.

	  If you want MMC/SD/SDIO support, you should say Y here and
	  also to your specific host controller driver.

config MMC_DEBUG
	bool "MMC debugging"
	depends on MMC != n
	help
	  This is an option for use by developers; most people should
	  say N here.  This enables MMC core and driver debugging.

config EMMC_HS200_SUPPORT
	bool "MMC card support HS200 mode"
	depends on MMC != n
	help
	  This is an optin for using HS200 in the kernel stage.

config EMMC_HS400_SUPPORT
	bool "MMC card support HS400 mode"
	depends on MMC != n && EMMC_HS200_SUPPORT && MSDC_USE_NEW_TUNE
	help
	  This is an optin for using HS400 in the kernel stage.

if MMC

source "drivers/mmc_common/core/Kconfig"

source "drivers/mmc_common/card/Kconfig"

source "drivers/mmc_common/host/Kconfig"

endif # MMC
